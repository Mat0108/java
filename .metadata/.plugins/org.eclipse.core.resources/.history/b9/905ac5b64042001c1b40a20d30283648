public class Equipe {
	private String nom;
	private String equipe;
	private int score;

	public Equipe(String nomequipe,String sport) {
		this.nom = nomequipe;
		this.equipe = sport;
		this.score = 0;
	}
	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		this.nom = nom;
	}

	public int getScore() {
		return score;
	}


	public void setScore(int score) {
		this.score = score;
	}

	public void ajouter(int x) {
		this.score += x;
	}

}
